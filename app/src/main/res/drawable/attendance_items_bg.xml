<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:top="@dimen/_3sdp"
        android:height="@dimen/_45sdp"
        android:width="@dimen/_150sdp"
        android:left="@dimen/_35sdp">

        <shape>
            <solid android:color="@color/dullgrey" />
            <stroke
                android:width="@dimen/_2sdp"
                android:color="@color/blue"/>
        </shape>
    </item>

    <item
        android:height="@dimen/_50sdp"
        android:width="@dimen/_50sdp">
        <shape android:shape="oval">

            <solid android:color="@color/appblue" />
            <stroke android:color="@color/blue"
                android:width="@dimen/_1sdp"/>
        </shape>
    </item>

</layer-list>
